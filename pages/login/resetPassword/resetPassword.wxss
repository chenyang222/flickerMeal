/**regist.wxss**/
.fl{
  float: left;
}
.fr{
  float: right;
}
.oh{
  overflow: hidden;
}
.my_ipt{
  width: 600rpx;
  margin: 0 auto;
  margin-top: 70rpx;
}
.my_ipt view{
  width: 100%;
  overflow: hidden;
  margin-top: 55rpx;
  padding-bottom: 24rpx;
  border-bottom: 1px solid #cfcfcf; 
}
.my_ipt view image{
  display: block;
  width: 27rpx;
  height: 35rpx;
  margin: 15rpx 30rpx 0rpx 30rpx;
}
.my_ipt view input{
  display: block;
  width: 500rpx;
  height: 35rpx;
  font-size: 30rpx;
  border: 0;
  outline: none;
  margin-top: 7rpx;
}
.my_ipt view .ipt_yzm{
  display: block;
  width: 305rpx;
  height: 35rpx;
  line-height: 35rpx;
  font-size: 30rpx;
  margin-top: 7rpx;
  border: 0;
  outline: none;
}
.yzm{
  width: 190rpx;
  height: 59rpx;
  line-height: 59rpx;
  text-align: center;
  font-size: 28rpx;
  color: #ff9e35;
  border: 1px solid #ff9f35;
  border-radius: 25px;
  -webkit-border-radius: 25px;
  -moz-border-radius: 25px;
}
.to_regist{
  width: 596rpx;
  height: 89rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  margin: 0 auto;
  margin-top: 140rpx;
}

/* 发送验证码按钮隐藏，并展示倒数60s提示 */
.hide{
 display: none;
}
.show{
 display: inline-block;
}


































