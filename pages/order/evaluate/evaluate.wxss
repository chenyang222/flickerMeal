/* pages/shopCar/index.wxss */
@import "../../../style/public.wxss";
/* @import "../../../style/weui.wxss"; */

page {
  background-color: #f8f8f8;
}
.pop {
  background-color: #000;
  opacity: 0.5;
  width: 100%;
  height: 100%;
  position: fixed;
  bottom: 0;
  z-index: 999;
}
.car-container {
}

.car-title {
  padding: 25rpx;
  /* position: relative; */
  background-color: #fff;
  margin-bottom: 1rpx;
  position: fixed;
  top: 0;
  z-index: 999;
  width: 93%;
  border-bottom: 1px solid #f7f7f7;
}

.goback {
  width: 20rpx;
  height: 34rpx;
  position: absolute;
  left: 25rpx;
  top: 35rpx;
}

.car-title text {
  display: inline-block;
  width: 100%;
  text-align: center;
}
.compa-icon{
  width:23rpx;
  height:26rpx;
}
.xing-list{
  width:30rpx;
  height:29rpx; 
}
.compa-title{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 25rpx;
  background-color: #fff;
  border-bottom: 1px solid #b3b3b3;
}
.compa-name{
  font-size:30rpx;
  font-family:SourceHanSansCN-Medium;
  color:rgba(45,45,45,1);
  margin-left: 10rpx;
}
.evaluate-list{
  padding: 25rpx;
  background-color: #fff;
}
.evaluate-list1{
  display: flex;
  flex-direction: row;
  justify-content: space-between
}
.evaluate-list2{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding: 15rpx 0;
}
.evaluate-img{
  width:153rpx;
  height:133rpx; 
  border-radius: 15rpx;
}
.upload-img{
  display: inline-block;
  width:153rpx;
  height:133rpx; 
  border-radius: 10rpx; 
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}
.meal-name{
  font-size:32rpx;
  font-family:SourceHanSansCN-Normal;
  color:rgba(51,51,51,1);
}
.evaluate-img-wrap{
  position: relative;
  display: inline-block;
  margin-right: 20rpx;
}
.del-img{
  position: absolute;
  right: -10rpx;
  top: -10rpx;
  width:33rpx;
  height:33rpx; 
}
.evaluate-txt-wrap{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  font-size:28rpx;
  font-family:SourceHanSansCN-Normal;
  color:rgba(102,102,102,1);
  margin: 10rpx 0;
}
.evaluate-input-txt{
  width:90%;
  height:51rpx; 
  border-radius: 10rpx ;
  border: 1px solid #e1e1e1;
  line-height: 51rpx;
  margin-top: -5rpx;
  margin-left: 10rpx;
}
.submit-btn{
  background-color: #ff9e35;
  height: 97rpx;
  line-height: 97rpx;
  font-size:32rpx;
  font-family:SourceHanSansCN-Regular;
  color:rgba(255,255,255,1);
  text-align: center;
}