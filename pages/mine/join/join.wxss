/* pages/mine/join/join.wxss */
.join{
  width: 100%;
  height: 500rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}
.joinbg{
  width: 100%;
  height: 1100rpx;
  position: relative;
  display:inline-block;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}
.invite{
  width: 310rpx;
  height: 47rpx;
  background-color: #ff5757;
  line-height: 47rpx;
  color: #fff;
  font-size: 20rpx;
  text-align: center;
  border-radius: 45rpx;
  /* margin:  auto; */
  margin-top: 230rpx;
  display: inline-block;
}
.transter{
  width: 310rpx;
  font-size: 15rpx;
  color: #aea496;
  margin-top: 20rpx;
}
.slogan{
  font-size: 18rpx;
  color: #6f5524;
  margin-top: 40rpx;
}
.joinus{
  /* position: absolute;
  top: 45rpx; */
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
  width: 367rpx;
  height: 70rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
  margin-left: 191.5rpx;
  margin-top: 35rpx;
}
.joinus text:first-child{
  font-size: 30rpx;
  color: #000000;
}
.joinus text:last-child{
  font-size: 14rpx;
  color: #d6d6d6;
}
.jointext{
  /* position: absolute;
  top: 140rpx; */
  width: 84%;
  /* left: 10%;  */
  margin: auto;
  font-size: 27rpx;
  text-indent: 40rpx;
  text-align: left;
  margin-top: 50rpx;
}

.joinuslist{
  width: 438rpx;
  height: 64rpx;
  border:2rpx solid #ff5757;
  border-radius: 50rpx;
  margin: auto;
  margin-top: 50rpx; 
  display: flex;
  line-height: 64rpx;
  
}
.joinuslist text{
  flex: 1;
  height: 58rpx;
  line-height: 58rpx;
  
  border-radius: 50rpx;
  font-size: 24rpx;
  text-align: center;
  margin-top: 3rpx;
}
.activebg{
  background-color: #eeeeee;  
  color: #333333;
}
.default{text-align:center;flex:1;color:#726b68;font-size:28rpx;}
.red{background-color: #eeeeee;color: #333333;}
.show{display:block;text-align:center;margin-top: 20rpx;}
.hidden{display:none;text-align:center;margin-top: 20rpx;}

.joinlist{
  width: 700rpx;
  height: 88rpx;
  line-height: 88rpx;
  margin:auto;
  display: flex;
  background-color: #feefe8;
  border-radius: 15rpx;
  margin-bottom: 30rpx;
  font-size: 24rpx;
}
.joinlist image{
  width: 50rpx;
  height: 35rpx;
  margin-top:27rpx;
  margin-left:15rpx;

}
.joinlist input{
    height: 100%;
    width: 600rpx;
    float: right;
    text-align: left;
    margin-left: 20rpx;
}


.recharge{
  width: 700rpx;
  margin: 0 auto;
  height: 89rpx;
  line-height: 89rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
  margin-top: 50rpx;
  font-size: 36rpx;
  color: #ffffff;
  text-align: center;
}