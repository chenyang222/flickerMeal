.aboutus{
  width: 100%;
  height: 1180rpx; 
  background-color: #f8f8f8;
  /* margin-top: 120rpx; */
}
.car-title {
  padding: 25rpx;
  /* position: relative; */
  background-color: #fff;
  margin-bottom: 1rpx;
  /* position: fixed;
  top: 0; */
  z-index: 999;
  width: 93%;
  border-bottom: 1px solid #f7f7f7;
}

.goback {
  width: 20rpx;
  height: 34rpx;
  position: absolute;
  left: 25rpx;
  top: 35rpx;
}

.car-title text {
  display: inline-block;
  width: 100%;
  text-align: center;
}

.aboutusbox{
  width: 698rpx;
  margin: auto;
  background-color: #fff;
}
.aboutuslogo{
  width: 100%;
  height: 57rpx;
  line-height: 57rpx;
  font-size: 28rpx;
  position: relative;
}
.aboutuslogo image{
  width: 30rpx;
  height: 30rpx;
  display: inline-block;
  position: absolute;
  top: 13.5rpx;
  left: 30rpx;
}
.aboutuslogo text{
  margin-left: 70rpx;
}
 .imgdata{
   width: 100%;
   height: 308rpx; 
   margin-top: 10rpx;
} 
.content{
  width: 95%;
  margin: auto;
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #666666;
}