/**index.wxss**/
@import "../../style/public.wxss";
.container{
    
}
.top{
        color: #ffffff;
        height:290rpx;
        width:100%;
        background-image: url("data:image/png;base64,");
        background-size:100% 100%;
}
.top .header{
  line-height: 88rpx;
  height: 88rpx;
  display: flex;
  justify-content: space-between;
}
.top .header image{
  margin: 25rpx;
  height: 37rpx;
  width: 37rpx;
}
.top .name{
  font-size: 28rpx;
  line-height: 28rpx;
}
.top .topMsg{
  padding-top: 20rpx;
  font-size: 22rpx;
  line-height: 28rpx;
  width: 100%;
  display: flex;
}
.top .right{
  padding-top: 14rpx;
  margin-right: 26rpx;
  flex: 1;
  display: flex;
  justify-content: space-between;
}
.top .topMsg .img{
  margin-left: 26rpx;
  height: 100rpx;
  width: 100rpx;
}
.top .topMsg .img image{
  height: 95rpx;
  width: 95rpx;
}
.top .topMsg .money{
  text-align: center;
  width: 201rpx;
}
.top .topMsg .money image{
  margin: 1rpx 0;
  height: 25rpx;
  width: 25rpx;
  vertical-align: middle;
}
.top .topMsg .msg{
  font-size: 22rpx;
}
.top .topMsg .msg image{
  margin-left: 30rpx;
  width: 12rpx;
  height: 20rpx;
  vertical-align: middle;
}
.jifen{
  padding: 5rpx 0;
  margin-top: 20rpx;
  background-size:100% 100%;
  background-image: url(data:image/png;base64,); 
}


/**center**/
.center{
  padding-bottom: 100rpx;
}
.center .list{
 border-top: #eeeeee 1rpx solid;
}
.center .list .item{
  font-size: 28rpx;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
}
.center .list .item .item_left{
  width: 114rpx;
  height: 100%;
}
.center .list .item .item_left image{
  padding: 23rpx;
  width: 54rpx;
  height: 54rpx;
}
.center .list .item .item_center{
  flex: 1;
  border-bottom: #eeeeee 1rpx solid;
}
.center .list .item .item_right{
  border-bottom: #eeeeee 1rpx solid;
  padding-right: 30rpx;
}
.center .list .item .item_right image{
  width: 22rpx;
  height: 22rpx;
}
/**footer**/
.footer{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 98rpx;
  background:rgba(255,255,255,1);
  border-top: #eeeeee 1rpx solid;
}
.footer .list{
  display: flex;
  justify-content: space-around;
}
.footer .list .footer_item{
  margin-top: 6rpx;
  width: 100rpx;
}
.footer .list .footer_item .footer_img{
   text-align: center;
  height: 45rpx;
}
.footer .list .footer_item .footer_img image{
  margin: 0 auto;
  width: 45rpx;
  height: 45rpx;
}
.footer .list .footer_item .footer_title{
  text-align: center;
  margin-top: 12rpx;
  font-size: 19rpx;
}
.footer .list .footer_item .on{
  color: #FFC240;
}
