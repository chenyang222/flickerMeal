/* pages/mine/setsecret/setsecret.wxss */
.nav{width:100%;height:83rpx;display:flex;flex-direction:row;margin-top: 5rpx;background-color: #ffffff;}
.default{line-height:83rpx;text-align:center;flex:1;color:#B3B3B3;font-size:28rpx;}
.red{line-height:83rpx;text-align:center;color:#FFC240;flex:1;border-bottom:1px solid #FFC240;font-size:28rpx;}
.show{display:block;text-align:center;margin-top: 20rpx;}
.show>view{ width: 100%;background: #ffffff;height: 88rpx;line-height: 88rpx;font-size: 30rpx;color: #333333;border-bottom: 1rpx solid #f8f8f8;}
.show view .showright{width: 583rpx;height:100%;float: right;text-align: left;}
.hidden{display:none;text-align:center;margin-top: 20rpx;}
.hidden view{ width: 100%;background: #ffffff;height: 88rpx;line-height: 88rpx;font-size: 30rpx;color: #333333;border-bottom: 1rpx solid #f8f8f8;display: flex;}
.hidden view .showright{width: 583rpx;height:100%;float: right;text-align: left;}
.invisecode{
  display: flex;
  position: relative;
}
.button{
  width: 186rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border: none;
  border: 1rpx solid #FF9F35;
  border-radius: 30rpx;
  font-size: 30rpx;
  color: #FF9F35;
  background-color: #ffffff;
  margin-top: 16rpx;
  padding: 0;
  position: absolute;
  right: 39rpx;
}
.buttonsss{
  width: 186rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border: none;
  border-radius: 30rpx;
  font-size: 30rpx;
  margin-top: 16rpx;
  padding: 0;
  position: absolute;
  right: 39rpx;
  border:1px solid #ccc;
