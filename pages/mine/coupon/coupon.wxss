page{
  width: 100%;
  height: 100%;
}
.container{
 background-color: #dddddd;
 height: 100%;
}
.header{
   background-color: #ffffff;
  padding: 0 25rpx;
  line-height: 88rpx;
  height: 88rpx;
  display: flex;
  justify-content: space-between;
}
.header .back{
  width: 20rpx;
  height: 34rpx;
}
.header .add{
  width: 34rpx;
  height: 34rpx;
}
.header image{
  height: 100%;
  width: 100%;
}

.list{
  height: 100%;
  overflow-y: scroll;
  padding-top: 2rpx;
  width: 100%;
  margin-top: 10rpx;
 
}
.list .item{
  margin-top: 13rpx;
  background-color: #ffffff;
  width: 690rpx;
  margin-left: 31rpx;
  height: 201rpx;
  display: flex;
  justify-content: space-between;
  background-image: url("data:image/jpeg;base64,");
  background-size:100% 100%;
}
.list .item .itemLeft{
  width: 224rpx;
  height: 201rpx;
  line-height: 201rpx;
  text-align: center;
  margin-right: 27rpx;
}
.list .item .itemLeft image{
  width: 23rpx;
  height: 32rpx;
  padding: 77rpx 35rpx ;
}

.list .item .itemRight{
  flex: 1;
}
.list .item .itemRight .itemRightTitle{
  color: #262626;
  font-size: 28rpx;
  margin-top: 51rpx;
  margin-bottom: 22rpx;
}
.list .item .itemRight .term{
  display: flex;
  height: 16rpx;
  line-height: 16rpx;
  margin-bottom: 8rpx;
}
.list .item .itemRight .term .imgs{
  margin-right: 11rpx;
}
.list .item .itemRight .term .imgs image{
  float: left;
  width: 16rpx;
  height: 16rpx;
}
.list .item .itemRight .term .termTitle{
  font-size: 17rpx;
  color: #4A4A4A;
}
.list .item .itemRight .state{
  font-size: 24rpx;
  color: #8B2700;
  text-align: right;
  padding-right: 20rpx;
}
