<!--mine.wxml-->
<view class="container">
  <view class="top">
    <view class="header">
      <navigator url="../mine/msgnotification/msgnotification">
        <image src="{{imgdata}}/mine/cf_msg.png"></image>
      </navigator>
      <view class="topTitle">我的</view>
      <navigator url="../mine/setup/setup">
        <image src="{{imgdata}}/mine/cf_setting.png"></image>
      </navigator>
    </view>
    <view class='topMsg'>
      <view class='img'>
        <image src='{{imgdata}}/mine/cf_nologin.png'></image>
      </view>
      <view class='right'>
        <view class='name'>餐厅001</view>
        <view class='money'>
          <view class='money'>
            <image src='{{imgdata}}/mine/cf_qiandai.png'></image>
            <text> 余 额：¥ {{balance}}</text>
          </view>
          <view class='jifen'>
            <image src='{{imgdata}}/mine/cf_jifen.png'></image>
            <text> 积 分：{{score}}</text>
          </view>
        </view>
        <navigator url="../mine/permsg/permsg">
          <view class='msg' url="../mine/permsg/permsg">个人信息
            <image src='{{imgdata}}/mine/cf_go.png'></image>
          </view>
        </navigator>
      </view>

    </view>
  </view>
  <view class='center'>
    <view class='list'>
      <!-- <view class='item'> -->
      <navigator class='item' url="../mine/address/address">
        <view class='item_left'>
          <image src='{{imgdata}}/mine/cf_address.png'></image>
        </view>
        <view class='item_center'>我的收货地址</view>
        <view class='item_right'>
          <image src='{{imgdata}}/mine/cf_goright.png'></image>
        </view>
      </navigator>
      <!-- </view> -->
      <!-- <view class='item'> -->
      <navigator class='item' url="../mine/pay/pay">
        <view class='item_left'>
          <image src='{{imgdata}}/mine/cf_chong.png'></image>
        </view>
        <view class='item_center'>我的充值</view>
        <view class='item_right'>
          <image src='{{imgdata}}/mine/cf_goright.png'></image>
        </view>
        <!-- </view> -->
      </navigator>
      <navigator class='item' url="../mine/coupon/coupon">
        <view class='item_left'>
          <image src='{{imgdata}}/mine/cf_youhuiquan.png'></image>
        </view>
        <view class='item_center'>我的优惠券</view>
        <view class='item_right'>
          <image src='{{imgdata}}/mine/cf_goright.png'></image>
        </view>
      </navigator>
      <navigator class='item' url="../mine/recommend/recommend">
        <view class='item_left'>
          <image src='{{imgdata}}/mine/cf_tuijian.png'></image>
        </view>
        <view class='item_center'>推荐有礼</view>
        <view class='item_right'>
          <image src='{{imgdata}}/mine/cf_goright.png'></image>
        </view>
      </navigator>
      <navigator class='item' url="../mine/store/store">
        <view class='item_left'>
          <image src='{{imgdata}}/mine/cf_shangcheng.png'></image>
        </view>
        <view class='item_center'>商 城</view>
        <view class='item_right'>
          <image src='{{imgdata}}/mine/cf_goright.png'></image>
        </view>
      </navigator>
      <navigator class='item' url="../mine/kefu/kefu">
        <view class='item_left'>
          <image src='{{imgdata}}/mine/cf_kefu.png'></image>
        </view>
        <view class='item_center'>客服管理</view>
        <view class='item_right'>
          <image src='{{imgdata}}/mine/cf_goright.png'></image>
        </view>
      </navigator>
      <navigator class='item' url="../mine/join/join">
        <view class='item_left'>
          <image src='{{imgdata}}/mine/cf_jiameng.png'></image>
        </view>
        <view class='item_center'>加盟</view>
        <view class='item_right'>
          <image src='{{imgdata}}/mine/cf_goright.png'></image>
        </view>
      </navigator>

      <navigator class='item' url="../mine/feedBack/feedBack" id='theid'>
        <view class='item_left'>
          <image src='{{imgdata}}/mine/cf_fankui.png'></image>
        </view>
        <view class='item_center'>反馈</view>
        <view class='item_right'>
          <image src='{{imgdata}}/mine/cf_goright.png'></image>
        </view>
      </navigator>

      <navigator class='item' url="../mine/feedBack/feedBack" id='theid'>
        <view class='item_left'>
          <image src='{{imgdata}}/mine/cf_jifenchoujiang.png'></image>
        </view>
        <view class='item_center'>积分抽奖</view>
        <view class='item_right'>
          <image src='{{imgdata}}/mine/cf_goright.png'></image>
        </view>
      </navigator>
    </view>
  </view>
</view>