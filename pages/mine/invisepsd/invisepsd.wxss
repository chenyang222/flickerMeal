/* pages/invisepad/invisepsd.wxss */
.invisephone{
  width: 100%;
  height: 100%;
  background-color: #f8f8f8;
}
.oldphone{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
  color: #333333;
  padding-left: 50rpx;
}
.import{
  width: 598rpx;
  margin: auto;
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  border-bottom: 1rpx solid #CFCFCF;
  margin-top: 20rpx;
}
.import input{
  width: 412rpx;
  height: 100%;
  border: none;
  font-size: 30rpx;
}
.import button{
  width: 186rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border: none;
  border: 1rpx solid #FF9F35;
  border-radius: 30rpx;
  font-size: 30rpx;
  color: #FF9F35;
  background-color: #ffffff;
  margin-top: 16rpx;
  padding: 0;
}
.rechargerule{
  width: 700rpx;
  text-align: right;
  font-size: 30rpx;
  color: #666666;
  margin-top: 30rpx;
}
.rechargerule image{
  width: 28rpx;
  height: 28rpx;
  margin-right: 11rpx;
}
.recharge{
  width: 700rpx;
  margin: 0 auto;
  height: 89rpx;
  line-height: 89rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
  margin-top: 50rpx;
  font-size: 36rpx;
  color: #ffffff;
  text-align: center;
}