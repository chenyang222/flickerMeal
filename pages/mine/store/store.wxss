/* pages/mine/feedBack/feedBack.wxss */
.store{
  width: 100%;
  height: 1180rpx; 
  background-color: #f8f8f8;
}
.car-title {
  padding: 25rpx;
  background-color: #fff;
  margin-bottom: 1rpx;
  z-index: 999;
  width: 93%;
  border-bottom: 1px solid #f7f7f7;
}

.goback {
  width: 20rpx;
  height: 34rpx;
  position: absolute;
  left: 25rpx;
  top: 35rpx;
}

.car-title text {
  display: inline-block;
  width: 100%;
  text-align: center;
}
.integrate{
  width: 100%;
  height: 129rpx;
  line-height: 129rpx;
  background-color: #FF9E35;
  border-top: 10rpx solid #f8f8f8;
  display: flex;
  justify-content: space-between;
  color: #fff;
}
.integrate text:first-child{
  font-size: 30rpx;
  margin-left: 30rpx;
}
.integrate text:last-child{
  font-size: 53rpx;
  margin-right: 30rpx;
}
.exchange{
  width: 100%;
  height: 163rpx;
  line-height: 163rpx;
  background-color: #f8f8f8;
  display: flex;
  justify-content: space-around;
  color: #fff;
}
.exchange text{
  width: 244rpx;
  height: 79rpx;
  line-height: 79rpx;
  border-radius: 45rpx;
  text-align: center;
  margin-top: 42rpx;
}
.exchange text:first-child{
  background-color: #AFAFAF;
}
.exchange text:last-child{
  background-color: #FF9E35;
}
.ticketlist{
  width: 100%;
}
.ticketbox{
  width: 100%;
  height: 157rpx;
  background-color: #ffffff;
  display: flex;
  position: relative;
  border-bottom: 10rpx solid #f8f8f8;
}
.ticketleft{
  width: 266rpx;
  height: 118rpx;
  display: flex;
  padding-top: 20rpx;
  margin-left: 24rpx;
}
.ticketleft view:first-child{
  width: 121rpx;
  height: 118rpx;
  text-align: center;
  /* line-height: 188rpx; */
  background-size: 100% 100%;
  
  background-image: url('data:image/png;base64,')
}
.ticketleft view:last-child{
  width: 145rpx;
  height: 118rpx;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,')
}
.ticketleft view:first-child .first{
  color: #FF9E35;
  font-size: 23rpx;
  display: inline-block;
  padding-top: 10rpx;
}
.ticketleft view:first-child .second{
  color: #FF9E35;
  font-size: 76rpx;
  display: inline-block;
  padding-top: 10rpx;
  
}
.ticketleft view:last-child .first{
  color: #fff;
  font-size: 23rpx;
  display: inline-block;
  padding-top: 10rpx;
  padding-left: 10rpx;
}
.ticketleft view:last-child .second{
  color: #fff;
  font-size: 22rpx;
  display: inline-block;
  padding-top: 10rpx;
  padding-left: 10rpx;
}
.ticketcenter{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  color: #2D2D2D;
  font-size: 28rpx;
  height: 118rpx;
  padding-top: 20rpx;
}
.ticketcenter text{
  margin-left: 30rpx;
}
.ticketright{
  width: 141rpx;
  height: 51rpx;
  text-align: center;
  line-height: 51rpx;
  background-color: #FF9E35;
  border-radius: 45rpx;
  color: #ffffff;
  font-size: 24rpx;
  position: absolute;
  top: 49rpx;
  right: 25rpx;
}