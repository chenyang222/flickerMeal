/* pages/mine/feedBack/feedBack.wxss */
.feedBack{
  width: 100%;
  height: 1180rpx; 
  background-color: #f8f8f8;
  /* margin-top: 120rpx; */
}
.car-title {
  padding: 25rpx;
  /* position: relative; */
  background-color: #fff;
  margin-bottom: 1rpx;
  /* position: fixed;
  top: 0; */
  z-index: 999;
  width: 93%;
  border-bottom: 1px solid #f7f7f7;
}

.goback {
  width: 20rpx;
  height: 34rpx;
  position: absolute;
  left: 25rpx;
  top: 35rpx;
}

.car-title text {
  display: inline-block;
  width: 100%;
  text-align: center;
}
.feedBack .comment{
  width: 95%;
  margin:  auto;
  height: 349rpx;
  background-color: #ffffff;
  border-radius: 5px;
  margin-top: 18rpx;
  overflow: hidden;
}
.feedBack .comment .textarea{
  width: 100%;
  height: 100%;
  padding-bottom: 174rpx;
  padding-top: 20rpx;
  padding-left: 20rpx;
  color: #999999;
  font-size: 30rpx;
}
.submitDo{
  width: 95%;
  height: 89rpx;
  margin: 38rpx  auto;
  line-height: 89rpx;
  background-color: #FF9E35;
  text-align: center;
  color: #ffffff;
  border-radius: 45rpx;
  
}