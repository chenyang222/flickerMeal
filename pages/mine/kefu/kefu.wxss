/* pages/mine/feedBack/feedBack.wxss */
.kefu{
  width: 100%;
  height: 1180rpx; 
  background-color: #f8f8f8;
}
.car-title {
  padding: 25rpx;
  background-color: #fff;
  margin-bottom: 1rpx;
  z-index: 999;
  width: 93%;
  border-bottom: 1px solid #f7f7f7;
}

.goback {
  width: 20rpx;
  height: 34rpx;
  position: absolute;
  left: 25rpx;
  top: 35rpx;
}

.car-title text {
  display: inline-block;
  width: 100%;
  text-align: center;
}
.kefulist{
  width: 100%;
  border-top: 10rpx solid #f8f8f8;
  background-color: #ffffff;
}
.listbox{
  width: 100%;
  height: 99rpx;
  line-height: 99rpx;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  /* padding: 0 30rpx; */
  border-bottom: 1rpx solid #D4D7DD;
}
.listleft{
  margin-left: 30rpx;
}
.listleft text:first-child{
  display: inline-block;
  width: 54rpx;
  height: 54rpx;
  line-height: 54rpx;
  border-radius: 50%;
  color: #ffffff;
  font-size: 20rpx;
  text-align: center;
  background-color: #FF855A;
}
.listleft text:last-child{
    margin-left: 30rpx;
}
.listright{
  margin-right:30rpx;
}
.listright text{
  margin-right: 30rpx;
}
.listright image{
  width: 30rpx;
  height: 30rpx;
}


/*电话遮罩  */
.nickname_center{
  width: 591rpx;
  height: 349rpx;
  background-color: #fff;
  border-radius:30rpx;
}
.nicktitle{
  width: 100%;
  height: 141rpx;
  line-height: 141rpx;
  color: #333333;
  font-size: 38rpx;
  text-align: center;
  font-weight: bold;
  display: block;
}
.nickinput{
  width: 464rpx;
  height: 60rpx;
  margin:auto;
  text-align: center;
  border-radius:10rpx;
}
.confirmbtns{
  width: 100%;
  height: 126rpx;
  line-height: 126rpx;
  border-radius: 0 0 30rpx 30rpx;
  display: flex;
  font-size: 38rpx;
   font-weight: 500; 
  border-top: 2rpx solid #F2A437;
  margin-top: 20rpx;
}
.confirmbtns text{
  flex: 1;
  text-align: center;
}
.confirmbtns text:first-child{
  color: #666666;
  border-right: 2rpx solid #F2A437;
}
.confirmbtns text:last-child{
  color: #F2A437;
}
.phcolor{
  font-size: 30rpx;
  color: #666666;
}